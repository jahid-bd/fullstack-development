{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "placedOn": {
      "type": "date"
    },
    "deliverOn": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "PLACED",
        "PROCESSING",
        "DELIVERED",
        "RETURNED"
      ]
    },
    "isCanceled": {
      "type": "boolean"
    },
    "usersId": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    }
  }
}
