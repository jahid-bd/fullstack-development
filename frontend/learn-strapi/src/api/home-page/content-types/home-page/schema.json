{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "Home Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "seoInformation": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-info"
    },
    "homeHero": {
      "displayName": "heroSection",
      "type": "component",
      "repeatable": false,
      "component": "layout.hero-section"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "callToAction": {
      "type": "string",
      "required": true
    },
    "blogCarousel": {
      "displayName": "PostSelection",
      "type": "component",
      "repeatable": false,
      "component": "blog.post-selection"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "SelectionHeading": {
      "type": "string"
    },
    "featuredPost": {
      "displayName": "featuredPost",
      "type": "component",
      "repeatable": false,
      "component": "blog.featured-post"
    },
    "featuredPostHeading": {
      "type": "string"
    },
    "post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post.post"
    },
    "BlogSelection": {
      "type": "dynamiczone",
      "components": [
        "blog.featured-post",
        "blog.post-selection"
      ],
      "required": true
    }
  }
}
