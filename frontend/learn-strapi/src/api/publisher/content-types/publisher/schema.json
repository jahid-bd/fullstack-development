{
  "kind": "collectionType",
  "collectionName": "publishers",
  "info": {
    "singularName": "publisher",
    "pluralName": "publishers",
    "displayName": "Publisher"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "bio": {
      "type": "text"
    },
    "books": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::book.book",
      "mappedBy": "publisherId"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "publishers"
    },
    "websiteLink": {
      "type": "string"
    }
  }
}
